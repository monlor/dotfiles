{
  "mcpServers": {
    "mcp-feedback-enhanced": {
      "command": "uvx",
      "args": ["mcp-feedback-enhanced@latest"],
      "env": {
        "MCP_DEBUG": "false",
        "MCP_WEB_PORT": "8765"
      },
      "autoApprove": ["interactive_feedback"]
    },
    "fetch": {
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ]
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y", "@upstash/context7-mcp"
      ]
    },
    "magic": {
      "command": "npx",
      "args": [
        "-y", "@21st-dev/magic@latest"
      ],
      "env": {
        "API_KEY": "$MAGIC_API_KEY"
      }
    },
    "taskmaster-ai": {
      "command": "npx",
      "args": [
        "-y", "--package=task-master-ai", "task-master-ai"
      ],
      "env": {
        "OPENAI_API_KEY": "$OPENAI_API_KEY"
      }
    },
    "playwright": {
      "command": "npx",
      "args": [
        "-y", "@playwright/mcp@latest"
      ]
    },
    "deepwiki": {
      "command": "npx",
      "args": [
        "-y", "mcp-deepwiki@latest"
      ]
    },
    "zen": {
      "command": "uvx",
      "args": [
        "--from", "git+https://github.com/BeehiveInnovations/zen-mcp-server.git", "zen-mcp-server"
      ],
      "env": {
        "OPENAI_API_KEY": "$OPENAI_API_KEY"
      }
    }
  }
}