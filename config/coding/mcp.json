{
  "mcpServers": {
    "mcp-feedback-enhanced": {
      "command": "uvx",
      "args": ["mcp-feedback-enhanced@latest"],
      "env": {
        "MCP_DEBUG": "false",
        "MCP_WEB_PORT": "8765"
      },
      "autoApprove": ["interactive_feedback"]
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y", "@upstash/context7-mcp"
      ]
    },
    "deepwiki": {
      "command": "npx",
      "args": [
        "-y", "mcp-deepwiki@latest"
      ]
    },
    "mcp-domain-availability": {
      "command": "uvx",
      "args": [
        "--from", "git+https://github.com/imprvhub/mcp-domain-availability",
        "mcp-domain-availability"
      ]
    },
    "supabase": {
      "command": "npx",
      "args": [
        "-y", "@supabase/mcp-server-supabase@latest"
      ],
      "env": {
        "SUPABASE_ACCESS_TOKEN": "$SUPABASE_ACCESS_TOKEN"
      }
    },
    "chrome-devtools": {
      "command": "npx",
      "args": [
        "chrome-devtools-mcp@latest",
        "--browser-url=http://127.0.0.1:9222"
      ]
    },
    "codex": {
      "command": "npx",
      "args": [
        "-y", "codex-mcp-server"
      ]
    }
  }
}
